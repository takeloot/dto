{
  "name": "@takeloot/dto",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/takeloot/dto.git",
  "author": "Aleksey Tsvetkov <code.aleksey@icloud.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "yarn build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "devDependencies": {
    "@nestjs/common": "^9.1.4",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@types/validator": "^13.7.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "rxjs": "^7.5.7",
    "ts-jest": "^29.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ]
  }
}